    background-color: #FFFFFF;
    color: #737373;
    border: none;
    border-radius: 0%;
    text-align-last: right;
    text-align: right;
    width: 35px;
    font-size: 16px;
    /* font-family: KanitBold; */
    font-weight: bold;
    font-family: supermarketregular !important;
}

.paginaSp {
    color: #F37D53 !important;
    font-size: 16px;
    font-weight: bold;
}

.paginationALL {
    background-color: #ffffff;
    color: #737373;
    border: none;
    border-radius: 0%;
    text-align-last: center;
    text-align: center;
    width: 35px;
    font-size: 16px;
    /* font-family: KanitBold; */
    font-weight: bold;
    font-family: supermarketregular !important;
}

